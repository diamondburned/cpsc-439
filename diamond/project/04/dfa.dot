digraph {
    node [shape=circle]
    rankdir = LR
    splines = true
    overlap = scale
    sep = 1

    i [label="" pos="-1,0!" shape=none width=0]
    0 [label="" pos=" 0,0!"]
    1 [label="" pos=" 1,0!"]
    2 [label="" pos=" 2,0!"]
    3 [label="" pos=" 3,0!"]
	4 [label="" pos=" 4,0!" peripheries=2]

	i    -> 0
	0    -> 1    [label="1"]
	0:n  -> 0:n  [label="0"]
	1    -> 2    [label="0"]
	1:n  -> 1:n  [label="1"]
	2    -> 3    [label="1"]
	2:sw -> 0:se [label="0"]
	3    -> 4    [label="1"]
	3:sw -> 2:se [label="0"]
}
